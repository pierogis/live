{
	"name": "@pierogis/utensils",
	"version": "0.3.1",
	"description": "a set of utensils to aid consumption",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/pierogis/utensils.git"
	},
	"license": "AGPL-3.0",
	"bugs": {
		"url": "https://github.com/pierogis/utensils/issues"
	},
	"homepage": "https://github.com/pierogis/utensils#readme",
	"keywords": [
		"svelte",
		"ui",
		"components"
	],
	"scripts": {
		"dev": "vite dev --port 3000",
		"build": "vite build",
		"package": "svelte-package",
		"preview": "vite preview",
		"test": "playwright test",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. .",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"lint:fix": "eslint --fix --ignore-path .gitignore ."
	},
	"devDependencies": {
		"@playwright/test": "^1.31.2",
		"@sveltejs/adapter-auto": "^2.0.0",
		"@sveltejs/kit": "^1.13.0",
		"@sveltejs/package": "^2.0.2",
		"@typescript-eslint/eslint-plugin": "^5.56.0",
		"@typescript-eslint/parser": "^5.56.0",
		"eslint": "^8.36.0",
		"eslint-config-prettier": "^8.8.0",
		"eslint-plugin-svelte3": "^4.0.0",
		"prettier": "^2.8.6",
		"prettier-plugin-svelte": "^2.10.0",
		"svelte": "^3.57.0",
		"svelte-check": "^3.1.4",
		"svelte-preprocess": "^5.0.3",
		"tslib": "^2.5.0",
		"typescript": "~5.0.2"
	},
	"dependencies": {
		"svelte": "^3.57.0"
	},
	"type": "module",
	"exports": {
		".": {
			"types": "./dist/index.d.ts",
			"svelte": "./dist/index.js",
			"default": "./dist/index.js"
		}
	},
	"files": [
		"dist",
		"!dist/demo"
	],
	"svelte": "./dist/index.js"
}
