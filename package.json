{
	"name": "@pierogis/careers",
	"version": "0.0.1",
	"description": "a starting point for working with pierogis",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/pierogis/careers.git"
	},
	"license": "AGPL-3.0",
	"bugs": {
		"url": "https://github.com/pierogis/careers/issues"
	},
	"homepage": "https://careers.pierogis.live",
	"keywords": [
		"svelte",
		"careers",
		"website"
	],
	"scripts": {
		"dev": "svelte-kit dev",
		"build": "svelte-kit build",
		"package": "svelte-kit package",
		"preview": "svelte-kit preview",
		"prepare": "svelte-kit sync",
		"test": "playwright test",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
	},
	"devDependencies": {
		"@pierogis/utensils": "^0.0.10",
		"@playwright/test": "^1.20.0",
		"@sveltejs/adapter-auto": "^1.0.0-next.36",
		"@sveltejs/kit": "^1.0.0-next.318",
		"@typescript-eslint/eslint-plugin": "^5.10.1",
		"@typescript-eslint/parser": "^5.10.1",
		"eslint": "^7.32.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^3.2.1",
		"prettier": "^2.5.1",
		"prettier-plugin-svelte": "^2.5.0",
		"svelte": "^3.44.0",
		"svelte-check": "^2.2.6",
		"svelte-preprocess": "^4.10.6",
		"tslib": "^2.3.1",
		"typescript": "~4.6.2"
	},
	"dependencies": {
		"@pierogis/utensils": "^0.0.7"
	},
	"type": "module"
