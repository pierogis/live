{
	"name": "plates",
	"version": "0.0.1",
	"scripts": {
		"dev": "svelte-kit dev",
		"build": "svelte-kit build",
		"vercel-build": "prisma generate && prisma migrate deploy && svelte-kit build && cp node_modules/@prisma/engines/*query* .vercel_build_output/functions/node/render/ && cp prisma/schema.prisma .vercel_build_output/functions/node/render/",
		"postinstall": "prisma generate",
		"prepare": "svelte-kit sync",
		"package": "svelte-kit package",
		"preview": "svelte-kit preview",
		"db:migrate": "prisma migrate",
		"db:seed": "prisma db seed",
		"db:reset": "prisma migrate reset",
		"check": "svelte-check --tsconfig ./tsconfig.json",
		"check:watch": "svelte-check --tsconfig ./tsconfig.json --watch",
		"lint": "prettier --ignore-path .gitignore --check --plugin-search-dir=. . && eslint --ignore-path .gitignore .",
		"format": "prettier --ignore-path .gitignore --write --plugin-search-dir=. ."
	},
	"prisma": {
		"seed": "node --experimental-specifier-resolution=node --loader ts-node/esm prisma/seeds/index.ts"
	},
	"devDependencies": {
		"@sveltejs/adapter-auto": "^1.0.0-next.33",
		"@sveltejs/kit": "^1.0.0-next.297",
		"@typescript-eslint/eslint-plugin": "^5.10.1",
		"@typescript-eslint/parser": "^5.10.1",
		"dotenv": "^16.0.0",
		"eslint": "^7.32.0",
		"eslint-config-prettier": "^8.3.0",
		"eslint-plugin-svelte3": "^3.2.1",
		"prettier": "^2.5.1",
		"prettier-plugin-svelte": "^2.5.0",
		"prisma": "^3.12.0",
		"svelte": "^3.44.0",
		"svelte-check": "^2.2.6",
		"svelte-preprocess": "^4.10.1",
		"ts-node": "^10.5.0",
		"tslib": "^2.3.1",
		"typescript": "~4.5.4"
	},
	"type": "module",
	"dependencies": {
		"@hapi/iron": "^6.0.0",
		"@prisma/client": "^3.12.0",
		"cookie": "^0.4.1",
		"nodemailer": "^6.7.2",
		"redis": "^4.0.4"
	}
}
