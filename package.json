	"description": "a starting point for working with pierogis",
	"repository": {
		"type": "git",
		"url": "git+https://github.com/pierogis/careers.git"
	},
	"license": "AGPL-3.0",
	"bugs": {
		"url": "https://github.com/pierogis/careers/issues"
	},
	"homepage": "https://careers.pierogis.live",
	"keywords": [
		"svelte",
		"careers",
		"website"
	],
	"scripts": {
		"dev": "vite dev --port 3000",
		"build": "vite build",
		"preview": "vite preview",
		"package": "svelte-package",
