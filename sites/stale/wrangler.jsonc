{
	"$schema": "node_modules/wrangler/config-schema.json",
	"name": "stale",
	"main": "./dist/_worker.js/index.js",
	"compatibility_date": "2025-08-07",
	"compatibility_flags": ["nodejs_compat"],
	"assets": {
		"binding": "ASSETS",
		"directory": "./dist"
	},
	"routes": [{ "pattern": "stale.pierogis.live", "custom_domain": true }],
	"observability": {
		"enabled": true
	}
}
